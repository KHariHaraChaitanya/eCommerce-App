<!DOCTYPE html>
<head>
showing results for search
</head>
<body>
  <div>
         <table class="table table-striped table-hover">
          <thead>
              <tr>
      
                  <th>TemplateName</th>
                  <th>TemplateImage</th>
                  <th>Description</th>
                  <th>Category  </th>
                  <th>Brand   </th>
                  <th>Actions </th>
              </tr>
          </thead>
          <tbody>
              <% for(var i=0; i < search.length; i++) { %>  
              <tr>
                 
                  
                  <td><%= search[i].TemplateName %></td>
      <td> <video controls=";" src ="<%= search[i].TemplateImage %>" style="width:50px ; margin:5px auto;height:50px">   </td>
                  <td><%= search[i].TemplateDescription %></td>
                  <td><%= search[i].Categories %></td>
                  <td><%= search[i].Brand %></td>
                  <td><a id=edit href="/" onclick="dashboard.ejs" class="btn btn-primary btn-xs" ><span class="glyphicon glyphicon-pencil"></span></a></td>
                  <!-- <td><a data-placement="top" data-toggle="tooltip" title="Delete"><button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" ><span class="glyphicon glyphicon-trash"></span></button></a></td> -->
                </tr>
                <% } %>     

          </tbody>
      </table>
      </div>
</body>